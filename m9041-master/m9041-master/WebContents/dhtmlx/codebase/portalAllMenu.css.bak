html, body {
	overflow: hidden;
}

body, button, input, select, textarea {
	font-family: az_ea_font,"Segoe UI",wf_segoe-ui_normal,"Segoe WP",Tahoma,Arial,sans-serif;
	font-weight: 400;
}
a, abbr, acronym, address, applet, article, aside, audio, b, big, blockquote, body, canvas, caption, center, cite, code, dd, del, details, dfn, div, dl, dt, em, embed, fieldset, figcaption, figure, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, html, i, iframe, img, ins, kbd, label, legend, li, mark, menu, nav, object, ol, output, p, pre, q, ruby, s, samp, section, small, span, strike, strong, sub, summary, sup, table, tbody, td, tfoot, th, thead, time, tr, tt, u, ul, var, video {
	font-size: 100%;
	font-weight: inherit;
}
div {
	display: block;
}
ul {
	display: block;
	list-style-type: disc;
	margin-block-start: 1em;
	margin-block-end: 1em;
	margin-inline-start: 0px;
	margin-inline-end: 0px;
	padding-inline-start: 40px;
}
li {
	display: list-item;
	text-align: -webkit-match-parent;
}
a {
	color: #015cda;
	text-decoration: none;
}
a:-webkit-any-link {
	color: -webkit-link;
	cursor: pointer;
	text-decoration: none;
}
a:hover {
	color: -webkit-link;
	cursor: pointer;
	text-decoration: none;
}
input {
	-webkit-appearance: textfield;
	background-color: white;
	-webkit-rtl-ordering: logical;
	cursor: text;
	padding: 1px;
	border-width: 2px;
	border-style: inset;
	border-color: initial;
	border-image: initial;
}
button {
	font-size: 12px;
	background: 0 0;
	color: inherit;
}
button:focus {
	outline: none;
}
input, textarea, select, button {
	text-rendering: auto;
	color: initial;
	letter-spacing: normal;
	word-spacing: normal;
	text-transform: none;
	text-indent: 0px;
	text-shadow: none;
	display: inline-block;
	text-align: start;
	margin: 0em;
	font: 400 13.3333px Arial;
}


.menu_content_layout {
	width: 100%;
}
.fxs-sidebar-browse-flyout {
	height: 100%;
}
.fxs-sidebar-content-wrapper {
	display: -webkit-flex;
	display: flex;
}
.fxs-sidebar-flyout.fxs-sidebar-improvedallservices .fxs-sidebar-content {
	padding-right: 10px;
}
.fxs-sidebar-flyout .fxs-sidebar-content {
	height: 66px;
	padding: 20px 0 20px 25px;
	display: -webkit-flex;
	display: flex;
	-webkit-flex: 1 1 auto;
	flex: 1 1 auto;
