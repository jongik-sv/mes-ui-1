<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><!DOCTYPE html><html>	<head>		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />		<title>MES Portal</title>				<link rel="stylesheet" href="/dhtmlx/codebase/portalContextMenu.css">				<script type="text/javascript" src="./dhtmlx/codebase/glue.ui.bootstrap.js"></script>				<script type="text/javascript">			$(document).ready(function() {				$("#wideView").click(function(e) {					parent.expandContentArea();					parent.hideContextMenu();				});				$("#menuView").click(function(e) {					parent.toggleSidebarMenu();					parent.hideContextMenu();				});				$("#closeAll").click(function(e) {					parent.closeAllTab();					parent.hideContextMenu();				});				$("#closeOthers").click(function(e) {					parent.closeOtherTabs();					parent.hideContextMenu();				});				$("#addFavorite").click(function(e) {					parent.addActiveTabToFavorite();					parent.hideContextMenu();				});				$("#removeFavorite").click(function(e) {					parent.removeActiveTabToFavorite();					parent.hideContextMenu();				});				$("#reloadTab").click(function(e) {					parent.reloadTab();				});			});		</script>	</head>	<body oncontextmenu="return false;" onselectstart="return false;" ondragstart="return false;">		<div class="menu">			<ul class="menu-options">			  <li id="wideView" class="menu-option">넓게 보기</li>			  <li id="menuView" class="menu-option b_seperator">메뉴 보기</li>			  <li id="reloadTab" class="menu-option">탭 다시열기</li>			  <li id="closeOthers" class="menu-option">다른 탭 닫기</li>			  <li id="closeAll" class="menu-option b_seperator">모두 닫기</li>			  <li id="addFavorite" class="menu-option">즐겨찾기 추가</li>			  <li id="removeFavorite" class="menu-option">즐겨찾기 삭제</li>			</ul>		  </div>	</body></html>