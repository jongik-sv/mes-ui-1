{
  "name": "mes-ui-framework",
  "version": "1.0.0",
  "description": "Modern MES UI Framework built with Vue.js 3 and PrimeVue",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0",
    "primevue": "^3.46.0",
    "primeicons": "^6.0.0",
    "primeflex": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.5.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "jsdom": "^23.0.0",
    "cypress": "^13.6.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-plugin-vue": "^9.19.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.0",
    "@types/node": "^20.10.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "primevue",
    "mes",
    "ui-framework",
    "typescript"
  ],
  "author": "MES UI Development Team",
  "license": "MIT"
}