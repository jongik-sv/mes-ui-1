<?xml version="1.0" encoding="UTF-8"?>
<service name="favorites-service" initial="Event_Router">
  <activity name="Seq 변경" class="com.poscoict.glue.biz.dhtmlx.form.FormUpdate">
    <transition name="success" value="스퀀스 정리" />
    <property name="sqlkey" value="favorites_afterDrag_update" />
    <property name="dao" value="m90dao" />
  </activity>
  <activity name="Event_Router" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="dragFind" value="UI DB 간 Seq 일치 여부 조회" />
    <transition name="add" value="즐겨찾기추가" />
    <transition name="find" value="favoritesFind" />
    <transition name="save" value="favoritesTreeSave" />
    <transition name="remove" value="즐겨찾기삭제" />
  </activity>
  <activity name="favoritesFind" class="com.posdata.glue.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="favoritesMenuResult" />
    <property name="dao" value="m90dao" />
    <property name="sqlkey" value="favorites_select" />
    <property name="param-count" value="1" />
    <property name="param0" value="USER_ID" />
  </activity>
  <activity name="즐겨찾기추가" class="com.poscoict.glue.biz.dhtmlx.form.FormInsert">
    <transition name="success" value="end" />
    <transition name="failure" value="end" />
    <property name="dao" value="m90dao" />
    <property name="sqlkey" value="favorites_insert" />
  </activity>
  <activity name="즐겨찾기삭제" class="com.poscoict.glue.biz.dhtmlx.form.FormDelete">
    <transition name="success" value="스퀀스 정리" />
    <property name="sqlkey" value="favorites_remove" />
    <property name="dao" value="m90dao" />
  </activity>
  <activity name="Error Message" class="com.unionsteel.mes.m90.common.SetUIMessage">
    <transition name="success" value="end" />
    <property name="msg" value="정보가 일치하지 않아 재조회 되었습니다.\n다시 시도하세요." />
    <property name="msgKey" value="errMsg" />
  </activity>
  <activity name="UI DB 간 Seq 일치 여부 조회" class="com.poscoict.glue.biz.dhtmlx.form.FormSearch">
    <transition name="error" value="Error Message" />
    <transition name="success" value="Seq 변경" />
    <property name="if-size-zero" value="error" />
    <property name="sqlkey" value="favorites_afterDrag_select" />
    <property name="dao" value="m90dao" />
  </activity>
  <activity name="favoritesTreeSave" class="com.poscoict.glue.biz.dhtmlx.PosTreeSave">
    <transition name="success" value="end" />
    <property name="dao" value="m90dao" />
    <property name="delete_sql" value="favorites_delete" />
    <property name="insert_sql" value="favorites_insert" />
  </activity>
  <transaction-manager id="tx1" commit="true" />
  <transaction-manager id="tx2" commit="true" />
  <activity name="스퀀스 정리" class="com.poscoict.glue.biz.dhtmlx.form.FormUpdate">
    <transition name="success" value="end" />
    <property name="sqlkey" value="favorites_organize_seq" />
    <property name="dao" value="m90dao" />
  </activity>
</service>

