/* [[[[ switch toggle button CSS */
.menu-flyout-toggle-wrapper {
	display: -webkit-flex;
	display: flex;
	-webkit-align-items: center;
	align-items: center;
	width: 250px;
	height: 25px;
	margin-left: 65px;
}
.menu-toggle-switch {
	position: relative;
	display: inline-block;
	width: 30px;
	height: 20px;
	margin: 0 0 0 5px;
}
.menu-toggle-switch input {
	display:none;
}
.menu-toggle-switch-slider {
	position: absolute;
	cursor: pointer;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background-color: #ccc;
	-webkit-transition: .4s;
	transition: .4s;
}
.menu-toggle-switch-slider:before {
	position: absolute;
	content: "";
	height: 12px;
	width: 12px;
	left: 4px;
	bottom: 4px;
	background-color: white;
	-webkit-transition: .4s;
	transition: .4s;
}
input:checked + .menu-toggle-switch-slider {
	background-color: #2196F3;
}
input:focus + .menu-toggle-switch-slider {
	box-shadow: 0 0 1px #2196F3;
}
input:checked + .menu-toggle-switch-slider:before {
	-webkit-transform: translateX(10px);
	-ms-transform: translateX(10px);
	transform: translateX(10px);
}
.menu-toggle-switch-slider.menu-toggle-switch-round {
	border-radius: 34px;
}
.menu-toggle-switch-slider.menu-toggle-switch-round:before {
	border-radius: 50%;
}
.menu-flyout-toggle-title-text {
	display: inline-block;
	font-size: 13px;
	font-weight: bold;
}
/* switch toggle button CSS ]]]] */